@model CandidaturaEmais.Models.Empresa

@{
    ViewData["Title"] = "Detalhes";
    ViewData["Subtitle"] = "Empresa";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4>@ViewData["Subtitle"]</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Representante)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Representante)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Contacto)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Contacto)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.email)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.email)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.EmpresaId">Alterar</a> |
    <a asp-action="Index">Voltar</a>
</div>
