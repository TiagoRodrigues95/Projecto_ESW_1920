@model CandidaturaEmais.Models.Tutor

@{
    ViewData["Title"] = "Detalhes";
    ViewData["Subtitle"] = "Tutor";

    ViewData["Edit"] = "Alterar";

    ViewData["Go Back"] = "Voltar";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4>@ViewData["Subtitle"]</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contacto)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contacto)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Empresa)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Empresa.Nome)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.TutorId">@ViewData["Edit"]</a> |
    <a asp-action="Index">@ViewData["Go Back"]</a>
</div>
